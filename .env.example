# supabase credentials
next_public_supabase_url=your_supabase_url_here
next_public_supabase_anon_key=your_supabase_anon_key_here
supabase_url=your_supabase_url_here
supabase_service_role_key=your_supabase_service_role_key_here

# database url for direct postgres access (if needed)
database_url=your_database_url_here

# jwt secret for supabase
supabase_jwt_secret=your_jwt_secret_here

# gmail oauth2 settings
email_aliases=email1@example.com,email2@example.com
from_name_map={"email1@example.com":"Name 1","email2@example.com":"Name 2"}
token_dir=./auth_tokens
client_secret_path=./auth_tokens/client_secret.json